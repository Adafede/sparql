#title: What are the compounds found in (children) organisms corresponding to a given ott_id?
SELECT DISTINCT ?structure ?structureLabel ?structure_inchi ?childrenLabel WHERE {
  VALUES ?ott_id {
    "332362"
    "1000758"
  }
  ?taxon wdt:P9157 ?ott_id.
  ?children (wdt:P171*) ?taxon.
  ?structure wdt:P234 ?structure_inchi;
    (p:P703/ps:P703) ?children.
  SERVICE wikibase:label { bd:serviceParam wikibase:language "en". }
}
LIMIT 10000